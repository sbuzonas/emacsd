(require 'package)
(add-to-list 'package-archives '("marmalade" . "http://marmalade-repo.org/packages/"))
(package-initialize)

(when (not package-archive-contents)
  (package-refresh-contents))

;; My Packages
(defvar my-packages
  '(clojure-mode
    clojurescript-mode
    find-file-in-git-repo
    gist
    magit
    nrepl
    paredit
    rainbow-mode
    smex))
